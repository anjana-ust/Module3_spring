package com.ust.rest.resource;

import java.math.BigDecimal;

import lombok.Getter;
import lombok.Setter;



public class Product {


	private long productId;
	private String productName;
	private String description;
	private BigDecimal price;
	private int qty;

	
	
	
	  public long getProductId() { return productId; } public void
	  setProductId(long productId) { this.productId = productId; } public String
	  getProductName() { return productName; } public void setProductName(String
	  productName) { this.productName = productName; } public String
	  getDescription() { return description; } public void setDescription(String
	  description) { this.description = description; } public BigDecimal getPrice()
	  { return price; } public void setPrice(BigDecimal price) { this.price =
	  price; } public int getQty() { return qty; } public void setQty(int qty) {
	  this.qty = qty; }
	 
	 
}
