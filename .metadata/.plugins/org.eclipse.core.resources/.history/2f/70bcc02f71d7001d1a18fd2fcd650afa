package com.ust.ioc;

public class TestLogin {
	public static void main(String[] args) {
		
	

	Login  login = new Login();
    login.setUserName("johndoe");
    login.setPassword("winnyminny");
    String result =login.validate();
    System.out.println(result);
    // object oriented programming is pull based approach
    // A class need to create instances on which it depends
    //spring ioc is based on push approach 
    //objects required by a class are injected with the help of configuration file
    
	}
}
